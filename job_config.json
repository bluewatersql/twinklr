{
  "schema_version": "2.0",
  "fixture_config_path": "fixture_config.json",

  "agent": {
    "max_iterations": 2,
    "token_budget": 75000,
    "enforce_token_budget": false,
    "success_threshold": 70,
    
    "plan_agent": {
      "model": "gpt-5.2",
      "temperature": 0.7
    },
    
    "implementation_agent": {
      "model": "gpt-5.2",
      "temperature": 0.5
    },
    
    "judge_agent": {
      "model": "gpt-5-mini",
      "temperature": 1.0
    },
    
    "refinement_agent": {
      "model": "gpt-5.2",
      "temperature": 0.7
    }
  },
  
  "planner_features": {
    "enable_shutter": true,
    "enable_color": true,
    "enable_gobo": true
  },

  "include_notes_track": true,
  "debug": true,
  "checkpoint": true
}

